<?xml version="1.0" encoding="UTF-8"?>
<model>
  <entity name="system-config" root="true">
    <element name="retry-times" value-type="int" />
    <entity-ref name="sql-flow-control" type="list" names="sql-flow-controls" />
    <entity-ref name="data-center" type="list" names="data-centers" />
  </entity>
  <entity name="sql-flow-control">
    <attribute name="sql-id" value-type="String" />
    <attribute name="allow-percent" value-type="int" />
    <attribute name="app" value-type="String" />
  </entity>
  <entity name="data-center">
    <attribute name="name" value-type="String" />
    <attribute name="ip-prefix" value-type="String" />
  </entity>
</model>

