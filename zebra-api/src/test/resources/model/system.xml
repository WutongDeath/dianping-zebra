<system-config>
	<retry-times>0</retry-times>
	<!-- 流控 -->
	<!-- key值表示每条sql的唯一id，value值表示允许打到数据库的比例，100%表示不拦截,database表示在哪个库上拦截 -->
	<sql-flow-control sql-id="aaa" allow-percent="10" app="group-service" database="192.168.1.1"/>
	<sql-flow-control sql-id="bbb" allow-percent="90" app="global" database="192.168.1.1,192.168.1.2"/>
</system-config>
