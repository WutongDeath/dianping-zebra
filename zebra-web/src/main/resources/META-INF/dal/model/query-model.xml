<?xml version="1.0" encoding="UTF-8"?>
<model model-package="com.dianping.zebra.admin.query"
	enable-base-visitor="true" enable-sax-parser="true" enable-json-parser="true"
	enable-xml-builder="true" enable-merger="true" enable-json-builder="true">
	<entity name="query" root="true">
		<element name="total" value-type="int" />
		<element name="max_score" value-type="double" />
		<entity-ref name="hits" type="list" names="hits" />
	</entity>
	<entity name="fields">
		<element name="path" value-type="String" />
		<element name="language" value-type="String" />
		<element name="code" value-type="String" />
		<element name="project_id" value-type="int" />
		<element name="name" value-type="String" />
	</entity>
</model>

