<?xml version="1.0" encoding="UTF-8"?>
<model>
	<entity name="query" root="true">
		<element name="total" value-type="int" />
		<element name="max_score" value-type="double" />
		<entity-ref name="hits" type="list" names="hitses" />
	</entity>
	<entity name="hits">
		<element name="_index" value-type="String" />
		<element name="_type" value-type="String" />
		<element name="_id" value-type="String" />
		<element name="_score" value-type="double" />
		<entity-ref name="fields" />
	</entity>
	<entity name="fields">
		<element name="path" value-type="String" />
		<element name="language" value-type="String" />
		<element name="code" value-type="String" />
		<element name="project_id" value-type="int" />
		<element name="name" value-type="String" />
	</entity>
</model>

